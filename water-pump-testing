void setup() {
  // put your setup code here, to run once:
Serial.begin (9600); //Starts the serial monitor
}

void loop() {
  // put your main code here, to run repeatedly:
Serial.println ("Mositure Level"); //Prints text to the serial monitor
Serial.println (analogRead(A0)); //Caputures a value between 0-900 to indicate how moist(lower numbers) or dry(higher numbers) the soil is
delay(2000); //Wait of 2 seconds. 1000 = 1 Second
}
